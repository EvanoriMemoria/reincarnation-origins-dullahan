{
  "type": "origins:multiple",
  "description": "this goes on the nightmare steed",
  "hidden": true,
  "glide": {
    "type": "origins:modify_falling",
    "velocity": 0.03,
    "take_fall_damage": false
  },
  "kill_on_dismount": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "particle soul_fire_flame ~ ~ ~ 1 1 1 0.4 50 normal"
        },
        {
          "type": "origins:execute_command",
          "command": "tp @s ~ ~-200 ~"
        },
        {
          "type": "origins:execute_command",
          "command": "kill @s"
        }
      ]
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:passenger",
          "comparison": "==",
          "compare_to": "0",
          "bientity_condition": {
            "type": "origins:actor_condition",
            "condition": {
              "type": "origins:entity_type",
              "entity_type": "minecraft:player"
            }
          }
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:lava",
          "comparison": ">",
          "compare_to": 0.4
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.4
        }
      ]
    }
  },
  "float_over_lava": {
    "type": "origins:walk_on_fluid",
    "fluid": "minecraft:lava",
    "condition": {
      "type": "origins:fluid_height",
      "fluid": "minecraft:lava",
      "comparison": "<=",
      "compare_to": 0.4
    }
  },
  "float_over_water": {
    "type": "origins:walk_on_fluid",
    "fluid": "minecraft:water",
    "condition": {
      "type": "origins:fluid_height",
      "fluid": "minecraft:water",
      "comparison": "<=",
      "compare_to": 0.4
    }
  },
  "particles": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle soul_fire_flame ~ ~1 ~ .7 .8 .7 0.01 1 normal"
    }
  }
}
